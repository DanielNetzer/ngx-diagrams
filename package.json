		"@angular/platform-browser-dynamic": "~10.1.6",
		"@angular/router": "~10.1.6",
		"core-js": "^3.6.5",
		"dagre": "^0.8.5",
		"rxjs": "~6.6.3",
		"tslib": "^2.0.3",
		"snyk": "^1.414.1",
		"zone.js": "~0.10.3"
	},
