    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "core-js": "^3.6.5",
    "dagre": "^0.8.5",
    "pathfinding": "^0.4.18",
    "paths-js": "^0.4.11",
    "rxjs": "~6.6.3",
    "snyk": "^1.419.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
